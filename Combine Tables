--Combine DEMO Tables
SELECT * INTO DEMO_COMBINED FROM 
(SELECT * FROM DEMO19Q1 UNION SELECT * FROM DEMO19Q2 UNION SELECT * FROM DEMO19Q3 UNION SELECT * FROM DEMO19Q4
UNION SELECT * FROM DEMO20Q1 UNION SELECT * FROM DEMO20Q2 UNION SELECT * FROM DEMO20Q3 UNION SELECT * FROM DEMO20Q4 
UNION SELECT * FROM DEMO21Q1 UNION SELECT * FROM DEMO21Q2 UNION SELECT * FROM DEMO21Q3 UNION SELECT * FROM DEMO21Q4 
UNION SELECT * FROM DEMO22Q1 UNION SELECT * FROM DEMO22Q2 UNION SELECT * FROM DEMO22Q3 UNION SELECT * FROM DEMO22Q4)a; 

--Combine DRUG Tables
SELECT * INTO DRUG_COMBINED FROM 
(SELECT * FROM DRUG19Q1 UNION SELECT * FROM DRUG19Q2 UNION SELECT * FROM DRUG19Q3 UNION SELECT * FROM DRUG19Q4
UNION SELECT * FROM DRUG20Q1 UNION SELECT * FROM DRUG20Q2 UNION SELECT * FROM DRUG20Q3 UNION SELECT * FROM DRUG20Q4 
UNION SELECT * FROM DRUG21Q1 UNION SELECT * FROM DRUG21Q2 UNION SELECT * FROM DRUG21Q3 UNION SELECT * FROM DRUG21Q4 
UNION SELECT * FROM DRUG22Q1 UNION SELECT * FROM DRUG22Q2 UNION SELECT * FROM DRUG22Q3 UNION SELECT * FROM DRUG22Q4)a; 

--Combine INDI Tables
SELECT * INTO INDI_COMBINED FROM 
(SELECT * FROM INDI19Q1 UNION SELECT * FROM INDI19Q2 UNION SELECT * FROM INDI19Q3 UNION SELECT * FROM INDI19Q4
UNION SELECT * FROM INDI20Q1 UNION SELECT * FROM INDI20Q2 UNION SELECT * FROM INDI20Q3 UNION SELECT * FROM INDI20Q4 
UNION SELECT * FROM INDI21Q1 UNION SELECT * FROM INDI21Q2 UNION SELECT * FROM INDI21Q3 UNION SELECT * FROM INDI21Q4 
UNION SELECT * FROM INDI22Q1 UNION SELECT * FROM INDI22Q2 UNION SELECT * FROM INDI22Q3 UNION SELECT * FROM INDI22Q4)a; 

--Combine OUTC Tables
SELECT * INTO OUTC_COMBINED FROM 
(SELECT * FROM OUTC19Q1 UNION SELECT * FROM OUTC19Q2 UNION SELECT * FROM OUTC19Q3 UNION SELECT * FROM OUTC19Q4
UNION SELECT * FROM OUTC20Q1 UNION SELECT * FROM OUTC20Q2 UNION SELECT * FROM OUTC20Q3 UNION SELECT * FROM OUTC20Q4 
UNION SELECT * FROM OUTC21Q1 UNION SELECT * FROM OUTC21Q2 UNION SELECT * FROM OUTC21Q3 UNION SELECT * FROM OUTC21Q4 
UNION SELECT * FROM OUTC22Q1 UNION SELECT * FROM OUTC22Q2 UNION SELECT * FROM OUTC22Q3 UNION SELECT * FROM OUTC22Q4)a; 

--Combine REAC Tables
SELECT * INTO REAC_COMBINED FROM 
(SELECT * FROM REAC19Q1 UNION SELECT * FROM REAC19Q2 UNION SELECT * FROM REAC19Q3 UNION SELECT * FROM REAC19Q4
UNION SELECT * FROM REAC20Q1 UNION SELECT * FROM REAC20Q2 UNION SELECT * FROM REAC20Q3 UNION SELECT * FROM REAC20Q4 
UNION SELECT * FROM REAC21Q1 UNION SELECT * FROM REAC21Q2 UNION SELECT * FROM REAC21Q3 UNION SELECT * FROM REAC21Q4 
UNION SELECT * FROM REAC22Q1 UNION SELECT * FROM REAC22Q2 UNION SELECT * FROM REAC22Q3 UNION SELECT * FROM REAC22Q4)a; 

--Combine RPSR Tables
SELECT * INTO RPSR_COMBINED FROM 
(SELECT * FROM RPSR19Q1 UNION SELECT * FROM RPSR19Q2 UNION SELECT * FROM RPSR19Q3 UNION SELECT * FROM RPSR19Q4
UNION SELECT * FROM RPSR20Q1 UNION SELECT * FROM RPSR20Q2 UNION SELECT * FROM RPSR20Q3 UNION SELECT * FROM RPSR20Q4 
UNION SELECT * FROM RPSR21Q1 UNION SELECT * FROM RPSR21Q2 UNION SELECT * FROM RPSR21Q3 UNION SELECT * FROM RPSR21Q4 
UNION SELECT * FROM RPSR22Q1 UNION SELECT * FROM RPSR22Q2 UNION SELECT * FROM RPSR22Q3 UNION SELECT * FROM RPSR22Q4)a; 

--Combine THER Tables
SELECT * INTO THER_COMBINED FROM 
(SELECT * FROM THER19Q1 UNION SELECT * FROM THER19Q2 UNION SELECT * FROM THER19Q3 UNION SELECT * FROM THER19Q4
UNION SELECT * FROM THER20Q1 UNION SELECT * FROM THER20Q2 UNION SELECT * FROM THER20Q3 UNION SELECT * FROM THER20Q4 
UNION SELECT * FROM THER21Q1 UNION SELECT * FROM THER21Q2 UNION SELECT * FROM THER21Q3 UNION SELECT * FROM THER21Q4 
UNION SELECT * FROM THER22Q1 UNION SELECT * FROM THER22Q2 UNION SELECT * FROM THER22Q3 UNION SELECT * FROM THER22Q4)a; 

--Combine all combined tables
SELECT * INTO COMBINED FROM  
(SELECT DEMO_COMBINED.primaryid,  
i_f_code, 
indi_pt,  
event_dt,  
mfr_dt, 
init_fda_dt, 
fda_dt,  
fda_dt, 
rept_cod, 
mfr_sndr,  
age,  
age_cod,  
sex,  
e_sub, 
wt,  
wt_cod,  
rept_dt, 
to_mfr, 
occp_cod,  
reporter_country,  
occr_country,  
role_cod, 
drugname,  
prod_ai,  
val_vbm, 
route_ad, 
dose_vbm, 
cum_dose_chr, 
cum_dose_unit, 
dechal, 
rechal, 
lot_num, 
exp_dt, 
nda_num, 
dose_amt, 
dose_unit, 
dose_form, 
dose_freq, 
pt, 
outc_cod, 
rpsr_cod, 
start_dt, 
end_dt, 
dur, 
dur_cod, 
indi_pt  
FROM DEMO_COMBINED JOIN DRUG_COMBINED ON DRUG_COMBINED.primaryid = DEMO_COMBINED.primaryid   
JOIN THER_COMBINED ON THER_COMBINED.primaryid = DRUG_COMBINED.primaryid  
JOIN INDI_COMBINED ON INDI_COMBINED.primaryid = DRUG_COMBINED.primaryid  
JOIN REAC_COMBINED ON REAC_COMBINED.primaryid = DEMO_COMBINED.primaryid  
JOIN OUTC_COMBINED ON OUTC_COMBINED.primaryid = DEMO_COMBINED.primaryid  
JOIN RPSR_COMBINED ON RPSR_COMBINED.primaryid = DEMO_COMBINED.primaryid  
)a  
; 
